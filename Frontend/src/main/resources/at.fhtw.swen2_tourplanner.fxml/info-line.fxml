<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" prefWidth="421.0" xmlns="http://javafx.com/javafx/16"
          fx:controller="at.fhtw.swen2_tourplanner.frontend.controller.InfoLineController">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" percentWidth="70.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="30.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="20" minHeight="20.0" prefHeight="20.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <Label fx:id="infoLabel" textFill="#008a00" GridPane.columnIndex="0">
        <padding>
            <Insets left="8.0" right="8.0"/>
        </padding>
        <font>
            <Font name="System Bold" size="10.0"/>
        </font>
    </Label>
    <ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="689.0" GridPane.columnIndex="1">
        <padding>
            <Insets left="8.0"/>
        </padding>
    </ProgressBar>
    <padding>
        <Insets right="2.0"/>
    </padding>
</GridPane>
